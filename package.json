{
  "name": "code-ocean-fechallenge",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:ui": "vite",
    "dev:db": "json-server --watch db/db.json --port 3001",
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:db\"",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^28.0.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  }
